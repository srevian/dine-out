generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/prisma-mongo/client"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model Resturant {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  Name    String
  Address String
  Ratings String?
  Reviews String[]
  Post    Bytes?
}
